{
  "expo": {
    "name": "Native Menu Planner",
    "slug": "Native_Menu_Planner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "menu-planner",
    "splash": {
      "image": "./assets/icons/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.svtroya.menuplanner"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.svtroya.menuplanner"
    },
    "web": {
      "favicon": "./assets/icons/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
